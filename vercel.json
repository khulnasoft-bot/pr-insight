{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "web/.next"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://localhost:8000/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@pr-insight-api-url"
  }
}
